{
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "python",
    "css",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "golang",
    "c",
    "cpp",
    "cc"
  ],

  
 

  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  
  }}
